<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Facebook Automation" parallel="false">

    <parameter name="cloud" value="false"/>
    <parameter name="envName" value="browserstack"/>
    <parameter name="platform" value="windows"/>
    <parameter name="browser" value="chrome"/>
    <parameter name="browserVersion" value="74"/>
    <parameter name="url" value="https://www.facebook.com"/>

    <test name="Facebook home">
        <classes>
            <class name="testfacebookhomepage.TestFacebookHomepage"></class>
        </classes>
    </test>
</suite>